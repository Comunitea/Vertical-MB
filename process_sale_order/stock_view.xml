<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_move_picking_form_add_comments" model="ir.ui.view">
            <field name="name">stock.move.form.add_commnets</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_picking_form"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="q_note"/>
                    <field name="det_note"/>
                </field>
            </field>
        </record>

        <record id="view_move_picking_tree_add_commnets" model="ir.ui.view">
            <field name="name">stock.move.tree.add_commnets</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_picking_tree"/>
            <field name="arch" type="xml">
                <field name="state" position="after">
                    <field name="q_note"/>
                    <field name="det_note"/>
                </field>
            </field>
        </record>
    </data>
</openerp>

<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_stock_config_settings_custom" model="ir.ui.view">
            <field name="name">stock settings custom</field>
            <field name="model">stock.config.settings</field>
            <field name="inherit_id" ref="stock.view_stock_config_settings"/>
            <field name="arch" type="xml">
                <separator string="Traceability" position="before">
                    <group string="Stock">
                        <label for="id" string="Stock Measures"/>
                        <div>
                          <div>
                              <label for= "configured_period_last_year"/>
                              <field name="configured_period_last_year" class="oe_inline"/>

                          </div>
                          <div>
                              <label for= "configured_period_adjustement"/>
                              <field name="configured_period_adjustement" class="oe_inline"/>
                          </div>
                        </div>
                    </group>
                </separator>
            </field>
         </record>
    </data>
</openerp>

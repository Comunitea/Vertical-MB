<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data>

        <!-- Parametres for importing folder -->
        <record id="edi_path_importation" model="ir.config_parameter">
            <field name="key">edi.path.importation</field>
            <field name="value">/media/depot_edi/in</field>
        </record>

        <!-- Parametres for exporting folder -->
        <record id="edi_path_exportation" model="ir.config_parameter">
            <field name="key">edi.path.exportation</field>
            <field name="value">/media/depot_edi/out</field>
        </record>

        <!-- Parametres for templates folder -->
        <record id="edi_path_templates" model="ir.config_parameter">
            <field name="key">edi.path.templates</field>
            <field name="value">/opt/BUILDOUTS/midban-depot/project-addons/depot_edi/wizard/templates</field>
        </record>

    </data>
</openerp>


<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    <record id="account_tax_code_form" model="ir.ui.view">
        <field name="name">account.tax.code.form</field>
        <field name="model">account.tax</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="account.view_tax_form" />
        <field name="arch" type="xml">
            <field name="description" position="after">
                <field name="code"/>
            </field>
        </field>
    </record>

    <record id="account_invoice_edi_form" model="ir.ui.view">
        <field name="name">account.invoice.edi.form</field>
        <field name="model">account.invoice</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="account.invoice_form"/>
        <field name="arch" type="xml">
            <page string="Payments" position="after">
                <page string="EDI">
                    <group>
                        <group>
                            <field name="name_doc"/>
                            <field name="file_name_doc"/>
                            <field name="date_doc"/>
                        </group>
                        <group>
                            <field name="state_doc"/>
                            <field name="date_process_doc"/>
                        </group>
                        <newline/>
                        <label string="XML file" colspan="4"/>
                        <field name="message" colspan="4" readonly="1" select="2" nolabel="1"/>
                    </group>
                </page>
            </page>
        </field>
    </record>

    <record id="account_invoice_supplier_edi_form" model="ir.ui.view">
        <field name="name">account.invoice.supplier.edi.form</field>
        <field name="model">account.invoice</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="account.invoice_supplier_form"/>
        <field name="arch" type="xml">
            <page string="Payments" position="after">
                <page string="EDI">
                    <group>
                        <group>
                            <field name="name_doc"/>
                            <field name="file_name_doc"/>
                            <field name="date_doc"/>
                        </group>
                        <group>
                            <field name="state_doc"/>
                            <field name="date_process_doc"/>
                        </group>
                        <newline/>
                        <label string="XML file" colspan="4"/>
                        <field name="message" colspan="4" readonly="1" select="2" nolabel="1"/>
                    </group>
                </page>
            </page>
        </field>
    </record>
</data>
</openerp>



<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record forcecreate="True" id="decimal_product_cost" model="decimal.precision">
            <field name="name">Product Cost</field>
            <field name="digits" eval="4"/>
        </record>

        <record forcecreate="True" id="ir_cron_recalculate_table" model="ir.cron">
            <field name="name">Recalculate Table</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'table.pricelist.prices'" name="model"/>
            <field eval="'recalculate_table'" name="function"/>
            <field eval="'(True,)'" name="args"/>
        </record>
    </data>
</openerp>
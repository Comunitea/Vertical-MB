        </record>
        <record id="thursday" model="supplier.service.days">
            <field name="name">Jueves</field>
        </record>
        <record id="friday" model="supplier.service.days">
            <field name="name">Viernes</field>
        </record>
        <record id="saturday" model="supplier.service.days">
            <field name="name">Sábado</field>
        </record>
        <record id="sunday" model="supplier.service.days">
            <field name="name">Domingo</field>
        </record>
    </data>
</openerp>
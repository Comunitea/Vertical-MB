              </div>
              <div class="row semiheight">
                  <div class="fullheight centerinside fullwidth">
                    <div class="centerthis">
                      <label for="customer_debt">Customer Debt:</label>
                      <input class=" " type="numeric" id='customer_debt' name="customer_debt"
                          t-att-value="widget.order_model.get('customer_debt').toFixed(2)" readonly="readonly" tabIndex="-1"/>
                  </div>
                </div>
              </div>
          </div>

          <div class="col-xs-2 fullheight">
                <div class="fullheight centerinside">
                    <div class="centerthis">
                      <label for="coment">Notes:</label>
                      <textarea class="semiheight notes mandatory" rows="2" cols="22" id='coment' name="coment"><t t-esc="widget.order_model.get('coment')"/></textarea>
                      <label for="customer_note">Customer Notes:</label>
                      <textarea class="semiheight notes" rows="2" cols="22" id='customer_comment' name="customer_comment"><t t-esc="widget.order_model.get('customer_comment')"/></textarea>
                    </div>
                </div>
          </div>
    </t>


<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data>
        <!-- Load a sequence for field default code. You can change it by
        anotherone but sequence mut be unique by product -->
    	<record id="sequence_type_product" model="ir.sequence.type">
            <field name="name">Product Numbers</field>
            <field name="code">product.serial.number</field>
        </record>
        <record id="sequence_product" model="ir.sequence">
            <field name="name">Product ref numbers</field>
            <field name="code">product.serial.number</field>
            <field name="prefix">PR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>